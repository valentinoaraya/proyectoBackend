<script defer src="/socket.io/socket.io.js"></script>
<script defer src="/js/index.js"></script>

<h1>Productos Tiempo Real:</h1>

<ul id="containerProducts">
    {{#each products}}
        <li>
            <p><b>{{title}}</b></p>
            <p>Precio: ${{price}}</p>
            <p>Descripción: {{description}}</p>
            <p>Stock: {{stock}}</p>
            <p>Categoría: {{category}}</p>
        </li>
        {{/each}}
</ul>